{
    "name": "mosen/munkireport-php-m2",
    "description": "MunkiReport PHP Experimental Fork",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "adldap2/adldap2-laravel": "^3.0",
        "cloudcreativity/laravel-json-api": "^0.6.2",
        "crazyinventor/laravel5-recaptcha2": "^1.3",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "rodneyrehm/plist": "^2.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^2.0",
        "tymon/jwt-auth": "^0.5.10",
        "webpatser/laravel-uuid": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Mr\\": "app/",
            "MrModule\\": "mr_module/",
            "MrLegacy\\": "mr_legacy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
